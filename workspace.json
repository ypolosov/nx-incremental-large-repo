        }
      }
    },
    "lib0-lib0": {
      "projectType": "library",
      "root": "libs/lib0/lib0",
      "sourceRoot": "libs/lib0/lib0",
      "prefix": "largerepo",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:ng-packagr-lite",
          "options": {
            "tsConfig": "libs/lib0/tsconfig.lib.json",
            "project": "libs/lib0/lib0/package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/lib0/tsconfig.lib.prod.json"
            }
          }
