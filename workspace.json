          "options": {
            "jestConfig": "libs/lib0/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/libs/lib0/lib0"]
        }
      },
      "schematics": {}
    },
    "lib0-childlib0": {
      "projectType": "library",
      "root": "libs/lib0/childlib0",
      "sourceRoot": "libs/lib0/childlib0",
      "prefix": "largerepo",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:ng-packagr-lite",
          "options": {
            "tsConfig": "libs/lib0/tsconfig.lib.json",
